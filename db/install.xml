<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/embedmanager/db" VERSION="2023071900" COMMENT="XMLDB file for Moodle local/embedmanager">
    <TABLES>
        <TABLE NAME="embed" COMMENT="Table to store embed data">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" COMMENT="Primary key"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" COMMENT="Name of the embed"/>
                <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="true" COMMENT="URL of the embed"/>
                <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="ID of the user who created this"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" COMMENT="Time when the record was created"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="embedmanager-user-foreign-key" TYPE="foreign" FIELDS="userid" REFTABLE="user" REFFIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
